{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts",
    "src/errors/",
    "src/clients/**/*.ts"
  ],
  "excludeInternal": true,
  "excludePrivate": true,
  "includeVersion": false,
  "treatWarningsAsErrors": true,
  "sort": [
    "required-first",
    "kind",
    "instance-first",
    "alphabetical"
  ]
}
