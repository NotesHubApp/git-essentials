    "@types/pako": "^2.0.0",
    "@types/sha.js": "^2.4.0",
    "jest": "^29.0.3",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-typescript": "^5.5.3",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
