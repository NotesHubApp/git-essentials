  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NotesHubApp/git-essentials/issues"
  },
  "homepage": "https://github.com/NotesHubApp/git-essentials#readme",
  "dependencies": {
    "async-lock": "^1.3.2",
    "buffer": "^6.0.3",
    "clean-git-ref": "^2.0.1",
